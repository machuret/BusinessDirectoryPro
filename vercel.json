{
  "version": 2,
  "functions": {
    "api/*.js": { 
      "runtime": "@vercel/node@3.0.7" 
    }
  },
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "/api/index" 
    },
    { 
      "src": "/health", 
      "dest": "/api/index" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/api/index" 
    }
  ]
}